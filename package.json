{
   "name": "nobull-filterapi",
   "version": "0.0.1",
   "description": "NoBull filter",
   "author": "Olatomide",
   "scripts": {
      "dev": "set debug=* && ts-node-dev -r dotenv/config -r tsconfig-paths/register --respawn  ./src/index.ts",
      "build": "gulp",
      "start": "npm run build && NODE_ENV=production pm2 start pm2.config.js",
      "dev:gulp": "nodemon --watch ./src -e 'ts' --exec \"gulp dev:build && node -r dotenv/config dist/index.js\"",
      "dev:esbuild": "nodemon --watch ./src -e 'ts' --exec \"etsc &&  node -r dotenv/config dist/index.js\"",
      "nodemon": "nodemon",
      "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
   },
   "devDependencies": {
      "@types/node": "^8.10.66",
      "babel-plugin-module-resolver": "^4.0.0",
      "del": "^5.1.0",
      "esbuild": "^0.9.0",
      "esbuild-node-tsc": "^1.2.0",
      "gulp": "^4.0.2",
      "gulp-mocha": "^7.0.2",
      "gulp-sourcemaps": "^2.6.5",
      "gulp-ts-alias": "^1.1.0",
      "gulp-typescript": "^6.0.0-alpha.1",
      "nodemon": "^2.0.6",
      "ts-node": "3.3.0",
      "ts-node-dev": "^1.1.6",
      "typescript": "^3.9.9"
   },
   "dependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.19.0",
      "@types/cors": "^2.8.8",
      "@types/helmet": "0.0.45",
      "@types/ioredis": "^4.17.7",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/mongoose": "^5.10.1",
      "axios": "^0.19.2",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-transformer": "^0.2.3",
      "connect-redis": "^4.0.4",
      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.15.4",
      "express-handlebars": "^5.2.0",
      "express-ip": "^1.0.4",
      "express-session": "^1.17.1",
      "helmet": "^3.23.3",
      "http": "0.0.1-security",
      "inversify": "^5.0.1",
      "ioredis": "^4.19.2",
      "jsonwebtoken": "^8.5.1",
      "mailgun-js": "^0.22.0",
      "moment": "^2.29.1",
      "moment-timezone": "^0.5.32",
      "mongoose": "^5.10.1",
      "passport": "^0.4.1",
      "passport-jwt": "^4.0.0",
      "passport-local": "^1.0.0",
      "passport-local-mongoose": "^6.0.1",
      "path": "^0.12.7",
      "pg": "^8.5.1",
      "qs": "^6.9.4",
      "rand-token": "^0.4.0",
      "reflect-metadata": "^0.1.10",
      "shortid": "^2.2.16",
      "stripe": "^8.160.0",
      "tsconfig-paths": "^3.9.0",
